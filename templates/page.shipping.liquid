<style>
    .shipping-page .accordion-button {
    font-size: 1.4rem;
    padding: 1.5rem 1.25rem;
        font-family: var(--headerfont);
    font-weight: var(--medium);
    text-transform: uppercase;
      
}
   input:focus, textarea:focus, select:focus{
        outline: none;
    }
.shipping-page .accordion-button:not(.collapsed)  {
color: var(--dark);

    background-color: #e5e5e4;
    box-shadow: inset 0 -1px 0 rgb(0 0 0 / 13%);
}
.shipping-page .accordion {
          margin: 0rem auto 9rem;
}

.shipping-page .accordion-collapse {

/*    background: #f8f9fa;*/
}
.shipping-page  .accordion-button:not(.collapsed)::after {
    background-image: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 16 16' fill='%23ffffff'%3e%3cpath fill-rule='evenodd' d='M1.646 4.646a.5.5 0 0 1 .708 0L8 10.293l5.646-5.647a.5.5 0 0 1 .708.708l-6 6a.5.5 0 0 1-.708 0l-6-6a.5.5 0 0 1 0-.708z'/%3e%3c/svg%3e");
