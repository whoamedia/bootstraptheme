<!doctype html>
<!--[if IE 9]> <html class="ie9 no-js supports-no-cookies" lang="{{ shop.locale }}"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html class="no-js supports-no-cookies" lang="{{ shop.locale }}">
<!--<![endif]-->

<head>
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="df-verify" content="dfe7f4af6c1dd9" />
    <meta content="width=device-width,initial-scale=1" name="viewport">
    <meta content="CheapUndies" name="author">
    <meta content="G21GuBHHzBqLhZB0qZldEjV10BwHbcuEfDZ38F6tMJ0" name="google-site-verification" />
    {%- render 'head-seo' -%}
    <meta content="index" name="robots">
    <meta content="follow" name="robots">
    {%- render 'head-social' -%}
    
    {%- comment -%} LCP Optimization: Preload Google Fonts instead of render-blocking @import {%- endcomment -%}
    <link rel="preconnect" href="https://fonts.googleapis.com" crossorigin>
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link rel="preload" as="style" href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700&display=swap" onload="this.onload=null;this.rel='stylesheet'">
    <noscript><link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700&display=swap"></noscript>
    
    {%- comment -%} Critical CSS: Load essential styles synchronously {%- endcomment -%}
    {{ 'bootstrap5.min.css' | asset_url | stylesheet_tag }}
    {{ 'base.css' | asset_url | stylesheet_tag }}
    {{ 'home.scss.css' | asset_url | stylesheet_tag }}
    
    {%- comment -%} Non-critical CSS: Defer icon fonts, custom styles, and animations {%- endcomment -%}
    <link rel="preload" as="style" href="https://pro-cdn.lineicons.com/3.0/regular/lineicons.css" onload="this.onload=null;this.rel='stylesheet'">
    <noscript><link rel="stylesheet" href="https://pro-cdn.lineicons.com/3.0/regular/lineicons.css"></noscript>
    <link rel="preload" href="{{ 'custom-css.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
    <noscript>{{ 'custom-css.css' | asset_url | stylesheet_tag }}</noscript>
    <link rel="preload" href="{{ 'components.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
    <noscript>{{ 'components.css' | asset_url | stylesheet_tag }}</noscript>
    <link rel="preload" href="{{ 'animations.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
    <noscript>{{ 'animations.css' | asset_url | stylesheet_tag }}</noscript>
    {% case template.name %}
      {% when 'index' %}
        {{ 'home.css' | asset_url | stylesheet_tag }}
      {% when 'collection' %}
        {{ 'collection.css' | asset_url | stylesheet_tag }}
      {% when 'product' %}
        {{ 'product.css' | asset_url | stylesheet_tag }}
      {% when 'cart' %}
        {{ 'cart.css' | asset_url | stylesheet_tag }}
      {% when 'blog' or 'article' %}
        {{ 'blog.css' | asset_url | stylesheet_tag }}
    {% endcase %}
    {% if template.directory == 'customers' %}
      {{ 'customer.css' | asset_url | stylesheet_tag }}
    {% endif %}
    {%- comment -%} Preload critical above-the-fold images {%- endcomment -%}
    <link rel="preload" as="image" href="{{ 'culogo@2x.png' | asset_url }}" fetchpriority="high">
    {% if template.name == 'index' %}
    <link rel="preload" as="image" href="{{ 'slide4.png' | asset_url }}" imagesrcset="{{ 'slide4.png' | asset_url }} 1600w" imagesizes="100vw" fetchpriority="high">
    {% endif %}
    {{ content_for_header }}
    {%- render 'loyaltylion' -%}
    <script src="{{ 'jquery.min.js' | asset_url }}" defer></script>
    <script src="{{ 'ls.rias.min.js' | asset_url }}" defer></script>
    <script src="{{ 'lazysizes.min.js' | asset_url }}" defer></script>
    <script src="{{ 'countdown-native.js' | asset_url }}" defer></script>

    {% if template.directory == 'customers' %}
    <script defer="defer" src="{{ 'shopify_common.js' | shopify_asset_url }}"></script>
    {% endif %}
    {{ shop.metafields.loox["global_html_head"] }}
    <script>
    let availableFilters = [];
    </script>
</head>

<body >
    {%- section 'my-cart' -%}
    {%- render 'slide-in-menu' -%}
    <!--   {%- include "feedback-bar" -%} -->
    {%- section "topbar" -%}
    {%- render "navbar" -%}
    <div class="container bg-white">
        {{ content_for_layout }}
    </div>
    {%- render "footer" -%}
    {%- if template.name == 'collection'-%}
    <script src="{{ 'mixitup.min.js' | asset_url }}" defer></script>
    <script src="{{ 'mixitup-multifilter.min.js' | asset_url }}" defer></script>
    <script src="{{ 'filters.js' | asset_url }}" defer></script>
    {%- elsif template.name == 'product' -%}
    <script src="{{ 'product.js' | asset_url }}" defer></script>
    {%- endif -%}
    {%- if template.name == 'collection' or template.name == 'blog' or template.name == 'product' or template.name == 'article' -%}
    <script>
    (function() {
      var addThisLoaded = false;
      function loadAddThis() {
        if (addThisLoaded) return;
        addThisLoaded = true;
        var script = document.createElement('script');
        script.src = '//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-4f85964f6c72421c';
        script.async = true;
        document.body.appendChild(script);
      }
      document.querySelectorAll('[class*="addthis_inline_share"]').forEach(function(el) {
        el.addEventListener('mouseenter', loadAddThis, { once: true });
        el.addEventListener('click', loadAddThis, { once: true });
        el.addEventListener('touchstart', loadAddThis, { once: true, passive: true });
      });
      if ('requestIdleCallback' in window) {
        requestIdleCallback(function() { setTimeout(loadAddThis, 5000); });
      } else {
        setTimeout(loadAddThis, 5000);
      }
    })();
    </script>
    {%- endif -%}
    <script src="{{ 'mmenu-light.js' | asset_url }}" defer></script>
    <script src="{{ 'menu.js' | asset_url }}" defer></script>
    <script src="{{ 'option_selection.js' | shopify_asset_url }}" defer></script>
    <script src="{{ 'bootstrap5.bundle.min.js' | asset_url }}" defer></script>



    <script type="application/json" data-ajax-cart-initial-state>
    {{ cart | json }}
    </script>
    <script type="module">
      import { subscribeToCartAjaxRequests } from '{{ "liquid-ajax-cart-v1.10.3.js" | asset_url }}';

      document.addEventListener('DOMContentLoaded', function() {
        subscribeToCartAjaxRequests((requestState, subscribeToResult) => {
          if (requestState.requestType === 'add') {
            const offcanvasEl = document.getElementById('offcanvasRight');
            if (offcanvasEl && window.bootstrap) {
              const bsOffcanvas = bootstrap.Offcanvas.getOrCreateInstance(offcanvasEl);
              bsOffcanvas.show();
            }

            const $ = window.jQuery || window.$;
            const itemEl = $ ? $("#AddToCart") : null;
            if (itemEl && itemEl.length) {
              itemEl.data('originalText', itemEl.html());
              itemEl.html('Added to Cart');
            }

            subscribeToResult((resultState) => {
              if (resultState.responseData?.ok) {
                const counterEl = document.getElementById('counter');
                const newCount = resultState.responseData.body?.item_count;
                if (counterEl && typeof newCount === 'number') {
                  counterEl.textContent = newCount;
                }

                const offcanvasElInner = document.getElementById('offcanvasRight');
                if (offcanvasElInner && window.bootstrap) {
                  const bsOffcanvas = bootstrap.Offcanvas.getOrCreateInstance(offcanvasElInner);
                  bsOffcanvas.show();
                }

                if (itemEl && itemEl.length) {
                  itemEl.html('<span class="icon-cart"></span> Add to Cart');
                }
              }
            });
          }
        });
      });
    </script>
    <script src="{{ 'app.js' | asset_url }}" defer></script>
    <script src="{{ 'lightbox-b5.js' | asset_url }}" defer></script>
    <script src="{{ 'midapi.js' | asset_url }}" defer></script>

	{{ shop.metafields.loox["global_html_body"] }}
</body>

</html>
