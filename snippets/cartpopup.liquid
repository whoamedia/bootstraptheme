<div aria-hidden="true" aria-labelledby="exampleModalCenterTitle" class="modal fade" id="cartpopup" role="dialog" tabindex="-1">
    <div class="modal-dialog modal-dialog-top" role="document">
        <div class="modal-content">
            <div class="modal-header d-flex align-items-center">
                <div class="modal-header-title">
                    <span class="align-middle">Your Shopping Cart</span>
                </div>
                <button aria-label="Close" class="btn-close me-2" data-bs-dismiss="modal" type="button"></button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-12 text-center">
                        <p class="hurrycart">
                            {% if customer.first_name != blank %}Hurry
                                {{ customer.first_name }}!
                            {% else %}Hurry!
                            {% endif %}
                            Before your cart expires!</p>
                        <form action="/cart" method="post" novalidate="">
                            <div class="row overlay ">
                                <div class="col-12 mt-md-0 cartdata">
                                    <div data-section-id="{{ section.id }}" data-section-type="cart-template" id="CartSection">
                                        <div class="table-responsive cart-table">
                                            <table class="table" data-cart-view="data-cart-view">
                                                <thead class="carttablebg">
                                                    <tr>
                                                        <th class="d-md-table-cell text-left" colspan="3" scope="col">Item</th>
                                                        <th class="d-none d-md-table-cell text-center" scope="col">Price</th>
                                                        <th class="text-center" scope="col" style="width: 20%">Quantity</th>
                                                        <th class="text-center" scope="col">Total</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr rv-each-item="cart.items">
                                                        <td class="align-middle d-none d-md-table-cell text-center">
                                                            <span class="plusminus">
                                                                <a class="icon-cross-circle" href="#" rv-data-cart-remove="index | plus 1"></a>
                                                            </span>
                                                        </td>
                                                        <td class="align-middle">
                                                            <a class='' rv-href='item.url'>
                                                                <img alt="item.title" class="img-fluid cartimg" rv-src="item.image | productImageSize '100x'" width="75px"/>
                                                            </a>
                                                        </td>
                                                        <td class="align-middle">
                                                            <a class='' rv-href='item.url'>
                                                                <span rv-text="item.title"></span>
                                                            </a>
                                                            <ul rv-hide="item.propertyArray | empty">
                                                                <li rv-each-property="item.propertyArray < properties">
                                                                    <small class="text-muted">{property.name}: {property.value}</small>
                                                                </li>
                                                            </ul>
                                                        </td>
                                                        <td class="d-none d-md-table-cell align-middle text-center" rv-html="item.price | money Currency.currentCurrency"></td>
                                                        <td class="d-table-cell d-sm-none align-middle text-center" colspan="2">
                                                            <span class="plusminus">
                                                                <a class="icon-circle-minus" href="#" rv-data-cart-quantity="item.quantity | minus 1" rv-data-cart-update="index | plus 1"></a>
                                                                <span>
                                                                    <span class="cartitemqty" rv-text="item.quantity"></span>
                                                                    <span class="plusminus">
                                                                        <a class="icon-plus-circle" href="#" rv-data-cart-quantity="item.quantity | plus 1" rv-data-cart-update="index | plus 1"></a>
                                                                    </span>
                                                                </td>
                                                                <td class="d-none d-md-table-cell align-middle text-center">
                                                                    <span class="plusminus">
                                                                        <a class="icon-circle-minus" href="#" rv-data-cart-quantity="item.quantity | minus 1" rv-data-cart-update="index | plus 1"></a>
                                                                        <span>
                                                                            <span class="cartitemqty" rv-text="item.quantity"></span>
                                                                            <span class="plusminus">
                                                                                <a class="icon-plus-circle" href="#" rv-data-cart-quantity="item.quantity | plus 1" rv-data-cart-update="index | plus 1"></a>
                                                                            </span>
                                                                        </td>
                                                                        <td class="align-middle text-center red" rv-html="item.line_price | money Currency.currentCurrency"></td>
                                                                    </tr>
                                                                    <tr class="align-middle text-center" rv-show="cart.item_count | lt 1">
                                                                        <td class="" colspan="6">
                                                                            <div class="image-empty-cart">
                                                                                <img alt="" class="img-fluid" src="data:image/png;base64,"/>
                                                                                <div class="text-empty-cart-1">SHOPPING CART IS</div>
                                                                                <div class="text-empty-cart-2">EMPTY</div>
                                                                            </div>
                                                                        </td>
                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                            <hr class=""/>
                                                        </div>
                                                    </div>
                                                </div>
                                              
                                                <div class="col-12 text-center" data-cart-view="data-cart-view">
                                                    <div class="cart_subtotal_header mb-3">
                                                        <div class="row">
                                                            <div class="col-6">
                                                                Subtotal
                                                            </div>
                                                            <div class="col-6" rv-html="cart.total_price | money Currency.currentCurrency"></div>
                                                        </div>
                                                    </div>
                                                    <button class="btn button button-3d button-rounded button-red" name="checkout" type="submit">
                                                      
                                                        {{ 'cart.general.checkout_continue' | t }}</button>
                                                   
                                                </div>
                                                
                                                 <div class="col-12 mt-0 mt-md-3 mb-1"  >
                                                        {{ content_for_additional_checkout_buttons }}</div>
                                                
                                                
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>